{
  "name": "random-interval-timer",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "vite build",
    "preview": "vite preview",
    "dev:vite": "vite",
    "pack": "npm run build && npx @electron/packager . --platform=win32 --arch=x64 --out=dist --overwrite --ignore=packaged --ignore=node_modules/.vite --ignore=.git --ignore=readme --icon=src/icon.ico --app-name=random-timer",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "timer",
    "electron",
    "random",
    "interval"
  ],
  "author": "",
  "license": "ISC",
  "description": "Random interval timer with a beep sound, ranging from a minimum of 1 second to a maximum of 99 hours, 59 minutes, and 59 seconds",
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "electron": "^37.2.1"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "canvas": "^3.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^7.0.4"
  }
}
