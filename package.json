{
  "name": "random-interval-timer",
  "version": "1.0.0",
  "main": "node/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "vite build",
    "preview": "vite preview",
    "dev:vite": "vite",
    "pack": "npm run build && npx @electron/packager . --platform=win32 --arch=x64 --out=dist --overwrite --ignore=packaged --ignore=node_modules/.vite --ignore=.git --ignore=readme --icon=node/icon.ico --app-name=random-timer",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "timer",
    "electron",
    "random",
    "interval"
  ],
  "author": "",
  "license": "ISC",
  "description": "Random interval timer with a beep sound, ranging from a minimum of 1 second to a maximum of 99 hours, 59 minutes, and 59 seconds",
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@eslint/js": "^9.31.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "electron": "^37.2.1",
    "eslint": "^9.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "canvas": "^3.1.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "styled-components": "^6.1.19",
    "vite": "^7.0.4"
  }
}
